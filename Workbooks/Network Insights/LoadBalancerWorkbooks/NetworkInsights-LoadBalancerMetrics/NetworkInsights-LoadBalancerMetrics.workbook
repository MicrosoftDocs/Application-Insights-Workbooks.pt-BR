                        "name": "LoadBalancer",
                        "label": "Load Balancer",
                        "type": 5,
                        "isRequired": true,
                        "value": null,
                        "typeSettings": {
                            "resourceTypeFilter": {
                                "Microsoft.Network/LoadBalancers": true,
                                "microsoft.network/loadbalancers": true
